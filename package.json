    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-async-script-loader": "^0.3.0",
    "react-bootstrap": "^2.6.0",
    "react-chartjs-2": "2.9.0",
    "react-copy-to-clipboard": "5.0.2",
    "react-csv-to-table": "^0.0.4",
    "react-datepicker": "^4.8.0",
    "react-datetime": "2.16.3",
