    "react-google-maps": "9.4.5",
    "react-google-places-autocomplete": "^3.4.0",
    "react-hook-form": "^7.37.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.4.0",
    "react-intl": "^6.1.1",
    "react-load-script": "0.0.6",
    "react-minimal-pie-chart": "^8.3.0",
    "react-minimal-side-navigation": "^1.9.2",
    "react-notify-toast": "^0.5.1",
    "react-oauth2-code-pkce": "^1.8.2",
