{
  "name": "argon-dashboard-react",
  "version": "1.1.0",
  "description": "React version of Argon Dashboard by Creative Tim",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/argon-dashboard-react.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "dashboard",
    "dashboard-react",
    "argon-dashboard",
    "argon-dashboard-react"
  ],
  "author": "Creative Tim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/argon-dashboard-react/issues"
  },
  "homepage": "",
  "scripts": {
    "start": "set HTTPS=true && env-cmd -f .env.dev react-scripts start",
    "start:dev": "env-cmd -f .env.dev react-scripts start",
    "start:stage": "env-cmd -f .env.stage react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "start:local": "env-cmd -f .env.local react-scripts start",
    "start:local-with-prod": "env-cmd -f .env.local.prod react-scripts start",
    "start:local-with-stage": "env-cmd -f .env.local.stage react-scripts start",
    "build:stage": "env-cmd -f .env.stage react-scripts build && sudo scp -i ~/Ai6/etows/ETowsStagging-Lightsail.pem -r build/* ubuntu@stage-console.etows.app:~/console/",
    "build:prod": "env-cmd -f .env.prod react-scripts build && sudo scp -i ../etows-app.pem -r build/* ubuntu@console.etows.app:/data/etows/console/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.12.1",
    "@hookform/resolvers": "^2.9.8",
    "@material-ui/core": "^4.12.4",
